<?xml version="1.0" encoding="utf-8"?>
<resources>

    <item name="repositoryUrl" type="config">http://192.168.0.100:9000/</item>

    <item name="repositoryPath" type="config">fishing/</item>

    <item name="repositorySearchUri" type="config">repo/search</item>

    <item name="repositoryLoadDomainInfoUri" type="config">repo/domain/</item>

    <item name="repositoryImagesUri" type="config">repo/images</item>

    <item name="repositoryUploadImagesDateFormat" type="config">yyDDDHHmmssSSS</item>

    <item name="mobileDateFormat" type="config">MMM d, HH:mm</item>

    <!-- Image thumbnail size scale factor related to larger display size -->
    <item name="thumbnailSizeScaleFactor" type="config">0.045</item>

    <!-- Image thumbnail border size scale factor related to thumbnail size -->
    <item name="thumbnailBorderSizeScaleFactor" type="config">0.04</item>

    <!-- In hours -->
    <item name="domainInfoSyncPeriodInHours" type="config">120</item>

    <item name="postponedPhotosThumbnailSizeScaleFactor" type="config">0.1</item>

    <item name="gridPhotosThumbnailSizeScaleFactor" type="config">0.13</item>

    <!--
        Possible values:
            MAP_TYPE_NONE = 0;
            MAP_TYPE_NORMAL = 1;
            MAP_TYPE_SATELLITE = 2;
            MAP_TYPE_TERRAIN = 3;
            MAP_TYPE_HYBRID = 4;
    -->
    <item name="mapType" type="config">3</item>

    <item name="cameraInitialZoom" type="config">9</item>

    <item name="debugTag" type="config">geoCatch</item>

</resources>

