    <item name="repositoryPath" type="config">fishing/</item>

    <item name="repositoryLoadImagesUri" type="config">spring/images/load/</item>

    <item name="repositoryUploadImagesUri" type="config">spring/images/upload</item>

    <item name="repositoryUploadImagesDateFormat" type="config">yyDDDHHmmssSSS</item>

